{% extends "base.html.twig" %}
{% block title %}Formulaire contact
{% endblock %}
{% block body %}
	<h1>Page Contact</h1>

	{{ form_start(formView) }}


	{{ form_errors(formView) }}

	{#{ form_widget(formView) }#}


	{{ form_row(formView.nom) }}
	{{ form_row(formView.prenom) }}
	{{ form_row(formView.telephone) }}
	{{ form_row(formView.email) }}
	{{ form_row(formView.message) }}

	<button type="submit" class="btn btn-primary">
		<i class="fas fa-save"></i>
		Envoyer
	</button>


	{{ form_end(formView) }}
{% endblock %}
